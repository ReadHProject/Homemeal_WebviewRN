{
  "expo": {
    "name": "Homemeal",
    "slug": "Homemeal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/hmlogoorangetransparent.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/hmlogoorangetransparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/hmlogoorangetransparent.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],

      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            { "scheme": "upi" },
            { "scheme": "intent" },
            { "scheme": "phonepe" },
            { "scheme": "paytm" },
            { "scheme": "tez" }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "homemeal.store",
              "pathPrefix": "/PhonePe/PaymentStatus"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],

      "package": "com.homemeal.store",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d3286deb-3c8b-485c-99f0-1e04a565c85c"
      }
    }
  }
}
